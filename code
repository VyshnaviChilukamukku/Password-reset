#include <stdio.h>
#include<string.h>
int main(void) {
char old[10],new[10],pass[]="nimm123#";
int n;
printf("Enter your old password:");
scanf("%s",old);
n=strcmp(old,pass);
if(n==0)
{
printf("\nPassword Matched");
printf("\nEnter new password:");
scanf("%s",new);
printf("\nPassword updated successfully");
printf("\nYour new password is %s",new);
}
else
printf("\nPassword doesnot match, please enter the correct password");
return 0;
}
